{
  "name": "@repo/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "seed": "tsx src/seed.ts",
    "check": "node -e \"require('dotenv').config(); if(!process.env.MONGODB_URI){throw new Error('MONGODB_URI missing')} console.log('OK')\""
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "mongoose": "^8.19.2",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
